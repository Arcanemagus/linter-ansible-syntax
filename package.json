{
  "name": "linter-ansible-syntax",
  "main": "./lib/main",
  "version": "1.1.0",
  "description": "Ansible linting via ansible-playbook --syntax-check",
  "repository": "https://github.com/mschuchard/linter-ansible-syntax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mschuchard/linter-ansible-syntax/issues"
  },
  "engines": {
    "atom": ">0.95.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-linter": "^4.7.0",
    "atom-package-deps": "^3.0.9"
  },
  "package-deps": [
    "linter",
    "language-ansible"
  ],
  "devDependencies": {
    "eslint": "latest",
    "babel-eslint": "latest"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "atom": true
    }
  }
}
